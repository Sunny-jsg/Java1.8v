package com.ait.in;

import java.util.Arrays;
import java.util.List;

public class Person {
	String name;
	int age;

	Person(String name, int age) {
		this.name = name;
		this.age = age;

	}

	public static void main(String[] args) {
		PredicatePersonDemo p0 = new PredicatePersonDemo("Ram", 20);
		PredicatePersonDemo p1 = new PredicatePersonDemo("Sonu", 15);
		PredicatePersonDemo p2 = new PredicatePersonDemo("Raju", 22);
		PredicatePersonDemo p3 = new PredicatePersonDemo("sunny", 21);
		PredicatePersonDemo p4 = new PredicatePersonDemo("Rani", 18);
		PredicatePersonDemo p5 = new PredicatePersonDemo("Manish", 12);
       List<PredicatePersonDemo> person = Arrays.asList(p0,p1,p2,p3,p4,p5);
	}
}
